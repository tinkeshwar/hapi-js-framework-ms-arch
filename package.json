{
  "name": "auth-service",
  "version": "1.0.0",
  "description": "Auth Service NodeJs: PustakHub",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "author": "Tinkeshwar Singh",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ext .ts src && tsc",
    "lint:stats": "./node_modules/.bin/eslint --format ./node_modules/eslint-stats/byError.js --ext .ts src",
    "lint:fix": "./node_modules/.bin/eslint --fix --ext .ts src",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/index.ts",
    "worker-dev": "cross-env NODE_ENV=development ts-node-dev --respawn --transpile-only src/libs/configs/worker.ts",
    "build": "./node_modules/.bin/tsc && copyfiles -u 1 \"src/**/*.pug\" dist/"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@bugsnag/js": "^7.16.2",
    "@hapi/hapi": "^20.2.2",
    "@hapi/inert": "^6.0.5",
    "@hapi/vision": "^6.1.0",
    "aws-sdk": "^2.1137.0",
    "bluebird": "^3.7.2",
    "bull": "^4.8.3",
    "busboy": "^1.6.0",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.0.1",
    "hapi-auth-jwt2": "^10.2.0",
    "hapi-swagger": "^14.5.0",
    "ioredis": "^5.0.5",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "mailgun-js": "^0.22.0",
    "mkdirp": "^1.0.4",
    "mysql2": "^2.3.3",
    "node-cleanup": "^2.1.2",
    "node-ses": "^3.0.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.19.1",
    "snakecase-keys": "^5.4.2",
    "to-snake-case": "^1.0.0",
    "url-join": "^5.0.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/bull": "^3.15.8",
    "@types/busboy": "^1.5.0",
    "@types/hapi__hapi": "^20.0.10",
    "@types/hapi__inert": "^5.2.3",
    "@types/hapi__vision": "^5.5.3",
    "@types/lodash": "^4.14.182",
    "@types/mailgun-js": "^0.22.12",
    "@types/mkdirp": "^1.0.2",
    "@types/node-cleanup": "^2.1.2",
    "@types/to-snake-case": "^1.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.15.0",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.4"
  }
